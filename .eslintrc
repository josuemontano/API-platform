{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  },
  "rules": {
    "camelcase": 0,
    "comma-dangle": 1,
    "curly": 0,
    "max-len": 0,
    "react/jsx-filename-extension": 0,
    "react/no-unknown-property": 0,
    "react/prop-types": 0
  }
}